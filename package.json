{
  "name": "@express/generator",
  "version": "1.0.0",
  "description": "express app starter",
  "main": "src/index.ts",
  "bin": "src/index.ts",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HRSHDG8/express-spring.git"
  },
  "keywords": [
    "express",
    "starter",
    "node",
    "cli"
  ],
  "author": "HRSHDG8",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HRSHDG8/express-spring/issues"
  },
  "homepage": "https://github.com/HRSHDG8/express-spring#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^8.3.0",
    "figlet": "^1.5.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
